{
    "name": "referenceapp-srv",
    "version": "1.0.0",
    "dependencies": {
        "@sap/cds": "^3.13.0",
        "express": "^4.17.1",
        "hdb": "^0.17.0",
        "@sap/xsenv": "^1.2.9",
        "passport": "^0.3.2",
        "@sap/xssec":"2.1.6",
        "@sap/hdbext":"5.1.0",
        "@sap/jobs-client": "1.3.6",
        "fast-csv": "^2.4.1",
        "multer": "^1.3.0",
        "jsonwebtoken": "8.3.0"
    },
    "engines": {
        "node": "^8.9",
        "npm": "^6"
    },
    "devDependencies": {},
    "scripts": {
        "postinstall": "npm dedupe && node .build.js",
        "start": "node server.js",
        "watch": "nodemon -w . -i node_modules/**,.git/** -e cds -x npm run build"
    },
    "private": true,
    "cds": {
        "requires": {
            "db": {
