{
	"welcomeFile": "ui/index.html",
	"authenticationMethod": "route",
	"routes": [{
		"source": "/catalog",
		"destination": "referenceapp-api",
		"authenticationType": "xsuaa"
	},{
		"source": "/csvImport",
		"destination": "referenceapp-api",
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.CSVImport.Edit"
	},{
		"source": "^/ui/(.*)$",
		"target": "$1",
		"localDir": "webapp",
		"authenticationType": "xsuaa"
	}]
}